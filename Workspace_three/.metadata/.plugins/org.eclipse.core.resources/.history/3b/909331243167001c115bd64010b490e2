package list1;

public class Vehicle {
	String make;
	String model;
	double price;
	boolean fourWDrive;
	
	public Vehicle(String make, String model, double price, boolean fourWDrive) {
		super();
		this.make = make;
		this.model = model;
		this.price = price;
		this.fourWDrive = fourWDrive;
	}
	
	
}
