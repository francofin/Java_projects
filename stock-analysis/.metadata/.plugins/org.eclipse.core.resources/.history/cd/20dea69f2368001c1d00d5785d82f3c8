package client;

public class Container {

	String item1;
	String item2;
	
	
	public Container(String item1, String item2) {
		super();
		this.item1 = item1;
		this.item2 = item2;
	}
	
	public void printItems() {
		System.out.println("contents");
		System.out.println("Item 1: "+ this.item1);
		System.out.println("Item 2: "+ this.item2);
	}
	
	
}
