package java_basics;

public class Variables {

	public static void main(String[] args) {
		int x;
		
		x= 4;
		
		int y = 20;
		
		int multiplied = x*y;
		System.out.println(multiplied);
		
		String words;
		
		words = "This is something I want to print out";
		
		System.out.println(words);

	}

}
