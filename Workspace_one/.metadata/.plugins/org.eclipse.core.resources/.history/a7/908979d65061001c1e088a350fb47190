package java_basics;

public class MyUtils {
	public static void printSomeStuff(String argument_variable) {
		System.out.println("HAHAHHAHA " + argument_variable);
	}
	
	public void printSomeStuff(int argument_variable) {
		System.out.println("NOOOO " + argument_variable);
	}
	
	public static void sumTwoNumbers(int firstArg, int secondArg) {
		int result = firstArg + secondArg;
		System.out.println(result);
	}
	
	public static int add10(int argNumber) {
		int result = 10+argNumber;
		return result;
	}
	
//	public static void sumTwoNumbers(int [] args) {
//		
//	}

}
